			<transient name="user" />
		</attributes>
	</entity>
	<entity class="com.wcs.newsletter.model.impl.SubscriptionCategoryImpl" name="SubscriptionCategory">
		<table name="WcsNewsletter_Subscription_Category" />
		<attributes />
	</entity>
